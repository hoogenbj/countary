<DialogPane prefWidth="350.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hoogenbj.countary.app.CloneBudgetDlgController">
    <content>
        <AnchorPane>
            <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <HBox spacing="10.0">
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </padding>
                    <Label alignment="CENTER_RIGHT" prefWidth="65.0" text="Name:" />
                    <TextField fx:id="name" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                </HBox>
            </VBox>
        </AnchorPane>
    </content>
    <ButtonType fx:id="okButtonType" buttonData="OK_DONE" text="OK" />
    <ButtonType buttonData="CANCEL_CLOSE" text="Cancel" />
</DialogPane>
